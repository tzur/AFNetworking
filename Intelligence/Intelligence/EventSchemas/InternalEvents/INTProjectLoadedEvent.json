{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Event marking a project load",
  "properties": {
    "project_id": {
      "description": "Project ID",
      "type": "string"
    },
    "is_new": {
      "description": "\\c YES if the project was loaded for the first time ever, on any device",
      "type": "boolean"
    }
  },
  "required": ["project_id", "is_new"]
}
