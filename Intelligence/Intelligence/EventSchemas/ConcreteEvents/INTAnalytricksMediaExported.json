{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A media object was exported",
  "type": "object",
  "allOf": [
    {"$ref": "../AbstractEvents/INTAnalytricksBaseUsage.json"},
    {
      "properties": {
        "export_id": {
          "description": "ID of the export operation",
          "type": "string",
          "pattern": "^[A-Fa-f0-9]{8}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{12}$"
        },
        "asset_type": {
          "description": "Asset type i.e. image, video",
          "type": "string"
        },
        "format": {
          "description": "Asset file format i.e. png, gif, mp4",
          "type": "string"
        },
        "asset_width": {
          "description": "Asset width, in pixels",
          "type": "integer",
          "minimum": 1
        },
        "asset_height": {
          "description": "Asset height, in pixels",
          "type": "integer",
          "minimum": 1
        },
        "asset_duration": {
          "description": "Asset length in seconds",
          "type": ["number", "null"],
          "minimum": 0
        },
        "export_target": {
          "description": "Target of the export. Examples: Camera Roll, Facebook",
          "type": "string"
        },
        "asset_id": {
          "description": "Unique Asset ID, as defined by the app",
          "type": "string"
        },
        "project_id": {
          "description": "Project id of the asset, null if the asset is not from a project",
          "type": ["string", "null"],
          "pattern": "^[A-Fa-f0-9]{8}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{12}$"
        },
        "is_successful": {
          "description": "True if the export was successful",
          "type": "boolean"
        }
      },
      "required": ["export_id", "asset_type", "format", "asset_width", "asset_height",
        "asset_duration", "export_target", "asset_id", "project_id", "is_successful"]
    }
  ]
}
